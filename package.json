{
  "name": "blaniel",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "mobile",
    "packages/*"
  ],
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' node server.js",
    "dev:next": "NODE_OPTIONS='--max-old-space-size=8192' next dev --turbopack",
    "dev:mobile": "cd mobile && npm start",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' prisma generate && NODE_OPTIONS='--max-old-space-size=8192' next build",
    "build:mobile": "cd mobile && npm run build",
    "start": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' node server.js",
    "start:next": "next start",
    "android": "cd mobile && npm run android",
    "ios": "cd mobile && npm run ios",
    "lint": "eslint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "db:prepare": "prisma generate && prisma db push --accept-data-loss",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:worlds": "tsx scripts/seed-predefined-worlds.ts",
    "db:seed:worlds:reset": "tsx scripts/seed-predefined-worlds.ts --reset",
    "db:seed:marilyn": "tsx scripts/seed-marilyn.ts",
    "db:seed:einstein": "tsx scripts/seed-einstein.ts",
    "minecraft:generate-configs": "tsx scripts/generate-minecraft-configs-batch.ts",
    "minecraft:migrate-manual": "tsx scripts/migrate-minecraft-configs-manual.ts",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "db:setup": "prisma generate && prisma db push --accept-data-loss && npm run db:seed",
    "monitor:emails": "tsx scripts/monitor-email-usage.ts",
    "test:email": "tsx -e \"import { sendTestEmail } from './lib/email'; sendTestEmail(process.argv[2]).then(() => console.log('âœ… Email sent!')).catch(console.error)\"",
    "encrypt-messages": "tsx scripts/encrypt-existing-messages.ts",
    "encrypt-messages:dry-run": "tsx scripts/encrypt-existing-messages.ts --dry-run",
    "admin:setup-ca": "./scripts/admin/setup-ca.sh",
    "admin:setup-totp": "tsx scripts/admin/setup-totp.ts",
    "admin:reset-totp": "tsx scripts/admin/reset-totp.ts",
    "admin:generate-cert": "tsx scripts/admin/generate-cert-cli.ts",
    "admin:list-certs": "tsx -e \"import { listCertificates } from './scripts/admin/cert-manager'; listCertificates(process.argv[2]).then(() => process.exit(0)).catch(e => { console.error(e); process.exit(1); })\"",
    "admin:revoke-cert": "tsx -e \"import { revokeCertificate } from './scripts/admin/cert-manager'; const [serial, reason] = process.argv.slice(2); revokeCertificate(serial, reason).then(() => process.exit(0)).catch(e => { console.error(e); process.exit(1); })\"",
    "admin:cleanup-certs": "tsx -e \"import { cleanupExpiredCertificates } from './scripts/admin/cert-manager'; cleanupExpiredCertificates().then(() => process.exit(0)).catch(e => { console.error(e); process.exit(1); })\"",
    "admin:update-crl": "tsx -e \"import { updateCRL } from './scripts/admin/cert-manager'; updateCRL().then(() => process.exit(0)).catch(e => { console.error(e); process.exit(1); })\"",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
